{
  "name": "frida-float-menu",
  "version": "1.0.0",
  "description": "A Frida library for creating interactive floating windows with UI components on Android",
  "license": "MIT",
  "private": false,
  "type": "module",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "files": [
    "src/**/*.ts",
    "README.md",
    "example.js"
  ],
  "scripts": {
    "dev": "frida-compile example.ts -o example-compiled.js -w",
    "frida": "frida -UF -l example-compiled.js",
    "build": "frida-compile example.ts -o example-compiled.js",
    "check": "tsc --noEmit",
    "clean": "rm -f _agent.js example-compiled.js"
  },
  "keywords": [
    "frida",
    "android",
    "ui",
    "floating-window",
    "overlay",
    "hooking"
  ],
  "devDependencies": {
    "@types/frida-gum": "^18.3.1",
    "@types/node": "^18.14.0",
    "frida-compile": "^19.0.4",
    "typescript": "^5.0.0"
  }
}
